        <activiti:field name="script">
          <activiti:string><![CDATA[var mail = actions.create("mail");
mail.execute(bpm_package);]]></activiti:string>
        </activiti:field>
        <activiti:field name="mail.parameters.to">
          <activiti:string><![CDATA[meilynlgg@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="mail.parameters.from">
          <activiti:string><![CDATA[testinnovacion01@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="mail.parameters.subject">
          <activiti:string><![CDATA[Notificacion de Resolucion]]></activiti:string>
        </activiti:field>
        <activiti:field name="mail.parameters.text">
          <activiti:string><![CDATA[Por medio del presente se le informa que su cooperativa ha sido registrada y se ha generado la Resolucion respectiva.]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow12" sourceRef="usertask7" targetRef="alfrescoMailtask2"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow13" sourceRef="alfrescoMailtask2" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="alfrescoMailtask1" targetRef="endevent1"></sequenceFlow>
