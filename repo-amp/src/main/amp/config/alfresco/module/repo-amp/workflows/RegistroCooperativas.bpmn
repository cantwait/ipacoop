      <extensionElements>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[if(task.getVariableLocal('ipawf_factibility') == true) {
	execution.setVariable('ipawf_respFact', 1);
}
else { 
	execution.setVariable('ipawf_respFact', 0);
}
if(task.getVariableLocal('ipawf_correct') == true) {
        execution.setVariable('ipawf_respCorr', 1);
 } else {
        execution.setVariable('ipawf_respCorr', 0);
}]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow3" sourceRef="usertask2" targetRef="usertask3"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="¿Existe estudio de factibilidad?" default="flow6"></exclusiveGateway>
