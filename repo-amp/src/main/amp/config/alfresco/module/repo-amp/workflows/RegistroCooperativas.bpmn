    <sequenceFlow id="flow6" name="SI" sourceRef="exclusivegateway1" targetRef="usertask4"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="usertask4" targetRef="parallelgateway1"></sequenceFlow>
    <serviceTask id="alfrescoMailtask1" name="Notificar a Dirección Provincial" activiti:class="org.alfresco.repo.workflow.activiti.script.AlfrescoScriptDelegate">
      <extensionElements>
        <activiti:field name="script">
          <activiti:string><![CDATA[var mail = actions.create("mail");
mail.execute(bpm_package);]]></activiti:string>
        </activiti:field>
        <activiti:field name="mail.parameters.to">
          <activiti:string><![CDATA[meilynlgg@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="mail.parameters.from">
          <activiti:string><![CDATA[testinnovacion01@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="mail.parameters.subject">
          <activiti:string><![CDATA[Notificacion de Rechazo]]></activiti:string>
        </activiti:field>
        <activiti:field name="mail.parameters.text">
          <activiti:string><![CDATA[Por medio del presente se le informa que su cooperativa no ha sido registrada por inconsistencia en la documentacion recibida.]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow8" name="NO" sourceRef="parallelgateway1" targetRef="alfrescoMailtask1"></sequenceFlow>
    <userTask id="usertask5" name="Inscribir Cooperativa " activiti:assignee="${initiator.properties.userName}" activiti:formKey="ipawf:submitInscription"></userTask>
    <sequenceFlow id="flow9" name="SI" sourceRef="parallelgateway1" targetRef="usertask5"></sequenceFlow>
    <userTask id="usertask6" name="Recibir y Firmar en Dirección Ejecutiva" activiti:assignee="${initiator.properties.userName}" activiti:formKey="ipawf:submitSign"></userTask>
    <sequenceFlow id="flow10" sourceRef="usertask5" targetRef="usertask6"></sequenceFlow>
    <userTask id="usertask7" name="Recibir en Dirección Provincial" activiti:assignee="${initiator.properties.userName}" activiti:formKey="ipawf:submitReceiveDirProv"></userTask>
    <sequenceFlow id="flow11" sourceRef="usertask6" targetRef="usertask7"></sequenceFlow>
